  test -z "${LIBM}" && LIBM=""
  ;;
*)
  # as with Apple, do NOT overwrite an existing value, even though
  # this time we are giving it one instead of clearing it:
  test -z "${LIBM}" && export LIBM="-lm"

  ;;
esac

