            ;;
	  *) # Check that GCC accepts it:
	    saved_CFLAGS="${CFLAGS}"
	    CFLAGS="${CFLAGS} ${w} -Werror"
	    saved_CXXFLAGS="${CXXFLAGS}"
	    CXXFLAGS="${CXXFLAGS} ${w} -Werror"
	    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
