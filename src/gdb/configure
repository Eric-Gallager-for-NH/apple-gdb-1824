	   # now look for Tk library stuff:
	   tkdir="../tk/${configdir}/"

	   TK_DEPS="${tkdir}${TK_LIB_FILE}"

	   # now look for Itcl library stuff:

#
# Ok, let us find the itcl configuration
# First, look for one uninstalled.
# the alternative search directory is invoked by --with-itclconfig
#

if test x"${no_itcl}" = x""; then
  # we reset no_itcl in case something fails here:
  no_itcl=true

# Check whether --with-itclconfig was given.
if test "${with_itclconfig+set}" = set; then :
  withval=$with_itclconfig; with_itclconfig=${withval}
fi

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Itcl configuration" >&5
$as_echo_n "checking for Itcl configuration... " >&6; }
  if ${ac_cv_c_itclconfig+:} false; then :
  $as_echo_n "(cached) " >&6
else


  # First check to see if --with-itclconfig was specified.
  if test x"${with_itclconfig}" != x""; then
    if test -f "${with_itclconfig}/itclConfig.sh" ; then
      ac_cv_c_itclconfig=`(cd ${with_itclconfig}; pwd)`
    else
      as_fn_error $? "${with_itclconfig} directory does NOT contain itclConfig.sh" "$LINENO" 5
    fi
  fi

  # then check for a private Itcl library:
  if test x"${ac_cv_c_itclconfig}" = x""; then
    for i in \
		../itcl/itcl \
		`ls -dr ../itcl[4-9]*/itcl 2>/dev/null` \
		../../itcl \
		`ls -dr ../../itcl[4-9]*/itcl 2>/dev/null` \
		../../../itcl \
		`ls -dr ../../../itcl[4-9]*/itcl 2>/dev/null`; do
      if test -f "$i/itclConfig.sh" ; then
        ac_cv_c_itclconfig=`(cd $i; pwd)`
	break
      fi
    done
  fi
  # check in a few common install locations:
  if test x"${ac_cv_c_itclconfig}" = x""; then
    for i in `ls -d ${prefix}/lib /usr/local/lib 2>/dev/null`; do
      if test -f "${i}/itclConfig.sh"; then
        ac_cv_c_itclconfig=`(cd ${i}; pwd)`
	break
      fi
    done
  fi
  # check in a few other private locations:
  if test x"${ac_cv_c_itclconfig}" = x""; then
    for i in \
		${srcdir}/../itcl/itcl \
		`ls -dr ${srcdir}/../itcl[4-9]*/itcl 2>/dev/null`; do
      if test -f "${i}/itclConfig.sh" ; then
        ac_cv_c_itclconfig=`(cd ${i}; pwd)`
	break
      fi
    done
  fi

fi

  if test x"${ac_cv_c_itclconfig}" = x""; then
    ITCLCONFIG="# no Itcl configs found"
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Cannot find Itcl configuration definitions" >&5
$as_echo "$as_me: WARNING: Cannot find Itcl configuration definitions" >&2;}
  else
    no_itcl=""
    ITCLCONFIG=${ac_cv_c_itclconfig}/itclConfig.sh
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: found ${ITCLCONFIG}" >&5
$as_echo "found ${ITCLCONFIG}" >&6; }
  fi
fi

           if test -z "${no_itcl}"; then


    if test -f "${ITCLCONFIG}"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: sourcing ${ITCLCONFIG}" >&5
$as_echo "$as_me: sourcing ${ITCLCONFIG}" >&6;}
      . $ITCLCONFIG
    fi



	     ITCLLIB="${ITCL_BUILD_LIB_SPEC}"
	     ITCL_DEPS="${ITCL_LIB_FULL_PATH}"
           else
             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: cannot load itclConfig.sh; it is missing" >&5
$as_echo "$as_me: WARNING: cannot load itclConfig.sh; it is missing" >&2;}
           fi

	   # now look for Itk library stuff:

#
# Ok, let us find the itk configuration
# First, look for one uninstalled.
# the alternative search directory is invoked by --with-itkconfig
#

if test x"${no_itk}" = x""; then
  # we reset no_itk in case something fails here:
  no_itk=true

# Check whether --with-itkconfig was given.
if test "${with_itkconfig+set}" = set; then :
  withval=$with_itkconfig; with_itkconfig=${withval}
fi

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Itk configuration" >&5
$as_echo_n "checking for Itk configuration... " >&6; }
  if ${ac_cv_c_itkconfig+:} false; then :
  $as_echo_n "(cached) " >&6
else


  # First check to see if --with-itkconfig was specified.
  if test x"${with_itkconfig}" != x""; then
    if test -f "${with_itkconfig}/itkConfig.sh"; then
      ac_cv_c_itkconfig=`(cd ${with_itkconfig}; pwd)`
    else
      as_fn_error $? "${with_itkconfig} directory does NOT contain itkConfig.sh" "$LINENO" 5
    fi
  fi

  # then check for a private Itk library:
  if test x"${ac_cv_c_itkconfig}" = x""; then
    for i in \
		../itcl/itk \
		`ls -dr ../itcl[4-9]*/itk 2>/dev/null` \
		../../itk \
		`ls -dr ../../itcl[4-9]*/itk 2>/dev/null` \
		../../../itk \
		`ls -dr ../../../itcl[4-9]*/itk 2>/dev/null` ; do
      if test -f "${i}/itkConfig.sh" ; then
        ac_cv_c_itkconfig=`(cd ${i}; pwd)`
	break
      fi
    done
  fi
  # check in a few common install locations:
  if test x"${ac_cv_c_itkconfig}" = x""; then
    for i in `ls -d ${prefix}/lib /usr/local/lib 2>/dev/null`; do
      if test -f "${i}/itkConfig.sh"; then
        ac_cv_c_itkconfig=`(cd ${i}; pwd)`
	break
      fi
    done
  fi
  # check in a few other private locations:
  if test x"${ac_cv_c_itkconfig}" = x""; then
    for i in \
		${srcdir}/../itcl/itk \
		`ls -dr ${srcdir}/../itcl[4-9]*/itk 2>/dev/null`; do
      if test -f "${i}/itkConfig.sh"; then
        ac_cv_c_itkconfig=`(cd ${i}; pwd)`
	break
      fi
    done
  fi

fi

  if test x"${ac_cv_c_itkconfig}" = x""; then
    ITKCONFIG="# no Itk configs found"
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Cannot find Itk configuration definitions" >&5
$as_echo "$as_me: WARNING: Cannot find Itk configuration definitions" >&2;}
  else
    no_itk=""
    ITKCONFIG=${ac_cv_c_itkconfig}/itkConfig.sh
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: found ${ITKCONFIG}" >&5
$as_echo "found ${ITKCONFIG}" >&6; }
  fi
fi

           if test -z "${no_itk}"; then


    if test -f "${ITKCONFIG}"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: sourcing ${ITKCONFIG}" >&5
$as_echo "$as_me: sourcing ${ITKCONFIG}" >&6;}
      . ${ITKCONFIG}
    fi



	     ITKLIB="${ITK_BUILD_LIB_SPEC}"
	     ITK_DEPS="${ITK_LIB_FULL_PATH}"
           else
             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: cannot load itkConfig.sh; it is missing" >&5
$as_echo "$as_me: WARNING: cannot load itkConfig.sh; it is missing" >&2;}
           fi

           ENABLE_CFLAGS="${ENABLE_CFLAGS} \$(SUBDIR_GDBTK_CFLAGS)"

	   # Include some libraries that Tcl and Tk want.
