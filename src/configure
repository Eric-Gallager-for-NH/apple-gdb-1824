  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: building out of tree but ${srcdir} (aka srcdir) contains host-${host_noncanonical}.
Use a pristine source tree when building in a separate tree" >&5
$as_echo "$as_me: WARNING: building out of tree but ${srcdir} (aka srcdir) contains host-${host_noncanonical}.
Use a pristine source tree when building in a separate tree" >&2;}
