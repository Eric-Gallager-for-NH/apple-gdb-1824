rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
	    CFLAGS="${saved_CFLAGS}"
	    CXXFLAGS="${saved_CXXFLAGS}"
            ;;
	esac
    done
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: using: ${WARN_CFLAGS}${WERROR_CFLAGS}" >&5
$as_echo "using: ${WARN_CFLAGS}${WERROR_CFLAGS}" >&6; }
    if test "x${BAD_WARN_CFLAGS}" != "x"; then
      test -n "${BAD_WARN_CFLAGS}"
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: compiler failed to accept: ${BAD_WARN_CFLAGS}" >&5
