if test x"${ac_cv_c_itclh}" != x""; then
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: found in ${ac_cv_c_itclh}" >&5
$as_echo "found in ${ac_cv_c_itclh}" >&6; }
     ITCLHDIR="-idirafter ${ac_cv_c_itclh}"
fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for Itk private headers. srcdir=${srcdir}" >&5
$as_echo_n "checking for Itk private headers. srcdir=${srcdir}... " >&6; }
if test x"${ac_cv_c_itkh}" = x""; then
  for i in ${srcdir}/../itcl ${srcdir}/../../itcl ${srcdir}/../../../itcl ${srcdir}/../itcl/itk; do
    if test -f ${i}/generic/itk.h ; then
      ac_cv_c_itkh=`(cd ${i}/generic; pwd)`
      break
    fi
  done
fi
if test x"${ac_cv_c_itkh}" = x""; then
  ITKHDIR="# no Itk private headers found"
  as_fn_error $? "Cannot find Itk private headers" "$LINENO" 5
fi
if test x"${ac_cv_c_itkh}" != x""; then
     { $as_echo "$as_me:${as_lineno-$LINENO}: result: found in ${ac_cv_c_itkh}" >&5
$as_echo "found in ${ac_cv_c_itkh}" >&6; }
     ITKHDIR="-idirafter ${ac_cv_c_itkh}"
