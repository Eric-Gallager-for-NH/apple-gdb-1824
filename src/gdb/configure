	# AIX 5 now supports IA64 processor
	lt_prog_compiler_static_GO='-Bstatic'
      fi
      lt_prog_compiler_pic_GO='-fPIC'
      ;;

    amigaos*)
      case $host_cpu in
      powerpc)
            # see comment about AmigaOS4 .so support
            lt_prog_compiler_pic_GO='-fPIC'
        ;;
      m68k)
